              "per_widget_wrapper_html" : "",
              "blog_listing_source" : "\n{# simple_list_page indicates the \"blog/all\" page, which is a list of links to every blog post #}\n<div class=\"cards{% if simple_list_page %}-simple{% endif %}\">\n    {% for content in contents %}\n      {% if loop.index > 1 %}\n        <div class=\"card\">\n            {% if not simple_list_page %}\n              <div class=\"blog-post-image\">\n                <!--post summary-->\n                {% if content.post_list_summary_featured_image %}\n                  <a href=\"{{content.absolute_url}}\" title=\"\" class=\"\">\n                      <img src=\"{{ content.post_list_summary_featured_image }}\" class=\"\" alt=\"{{ content.featured_image_alt_text | escape }}\">\n                  </a>\n                {% endif %}\n              </div>\n              <div class=\"blog-post-content\">\n                <div class=\"topic-wrap\">\n                     <p id=\"hubspot-topic_data\"> Whitepaper |\n                        {% for topic in content.topic_list %}\n                            {{ topic.name }}{% if not loop.last %}{% endif %}\n                        {% endfor %}\n                     </p>\n                   </div>\n                   <div class=\"post-header-wrap\">\n                    <h2><a href=\"{{content.absolute_url}}\">{{ content.name }}</a></h2>\n                   </div>\n                   <div class=\"post-content-wrap\">\n                    {{ content.post_list_content|safe|striptags|truncate(120, True, '.') }}\n                   </div>\n                   <div class=\"post-more-wrap\">\n                      {% if content_group.show_summary_in_listing %}\n                          <a class=\"more-link\" href=\"{{ content.absolute_url }}\">Read More <i class=\"fas fa-arrow-right\"></i></a>\n                      {% endif %}\n                   </div>\n              </div>\n                {% if content.topic_list %}\n                {% endif %}\n            {% else %}\n            {% endif %}\n        </div>\n      {% endif %}\n     {% endfor %}\n</div>\n\n\n{% if not simple_list_page %}\n<div class=\"blog-pagination\">\n    {% if last_page_num %}\n        <a class=\"previous-posts-link\" href=\"{{ blog_page_link(last_page_num) }}\">Previous</a>\n    {% endif %}\n        <a class=\"all-posts-link\" href=\"{{ blog_all_posts_url(group.id) }}\">All posts</a>\n    {% if next_page_num %}\n        <a class=\"next-posts-link\" href=\"{{ blog_page_link(next_page_num) }}\">Next</a>\n    {% endif %}\n</div>\n{% endif %}",
