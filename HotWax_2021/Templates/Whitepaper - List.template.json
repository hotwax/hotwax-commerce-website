              "path" : "@hubspot/blog_content",
              "per_widget_wrapper_html" : "",
              "blog_listing_source" : "<!-- whitepaper -->\n<div class=\"blog-section\">\n    {% for content in contents %}\n        {% if loop.index <= 1 %} <div class=\"blog-banner post-item\">\n            {% if not simple_list_page %}\n                {% if content.post_list_summary_featured_image %}\n                    <a href=\"{{content.absolute_url}}\" title=\"\" class=\"blog-banner-image\">\n                      <img src=\"{{ content.post_list_summary_featured_image }}\" class=\"\" alt=\"{{ content.featured_image_alt_text | escape }}\">\n                    </a>\n                {% endif %}\n                <div class=\"blog-banner-content\">\n                    <div class=\"topic-wrap\">\n                        <p id=\"hubspot-topic_data\"> Whitepaper |\n                            {% for topic in content.topic_list %}\n                            {{ topic.name }}{% if not loop.last %}{% endif %}\n                            {% endfor %}\n                        </p>\n                    </div>\n                    <div class=\"title-wrap\">\n                        <h1><a href=\"{{content.absolute_url}}\">{{ content.name }}</a></h1>\n                    </div>\n                    <div class=\"more-wrap\">\n                        {% if content_group.show_summary_in_listing %}\n                        <a class=\"more-link\" href=\"{{ content.absolute_url }}\">Read More <i\n                                class=\"fas fa-arrow-right\"></i></a>\n                        {% endif %}\n                    </div>\n\n                    <div class=\"download-whitepaper\">\n                        <a href=\"{{ content.widgets.your_pdf_field.body.value }}\">Download</a>\n                    </div>\n                </div>\n                {% else %}\n            {% endif %}\n        {% endif %}\n    {% endfor %}\n</div>",
